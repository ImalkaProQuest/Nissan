    <validationRules>
        <fullName>Opportunity_Status</fullName>
        <active>true</active>
        <errorConditionFormula>OR(TEXT(Opportunity__r.StageName) = &apos;Closed Lost&apos;, TEXT(Opportunity__r.StageName) = &apos;Discontinued&apos;)</errorConditionFormula>
        <errorMessage>Cannot add a Delivery Schedule to a Closed Lost or Discontinued Opportunity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Product_Level</fullName>
        <active>true</active>
        <errorConditionFormula>Vehicle__r.SFDC_Product_Level__c &lt;&gt; 1&amp;&amp; Vehicle__r.Do_Not_Enforce_Product_Level_Validation__c = FALSE</errorConditionFormula>
        <errorMessage>Select a vehicle at level 1 in the product hierarchy.</errorMessage>
    </validationRules>
